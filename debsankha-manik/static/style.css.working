@import url(http://fonts.googleapis.com/css?family=IM+Fell+DW+Pica);
/*
@import url(http://fonts.googleapis.com/css?family=Droid+Serif);
*/

html{
	width:100%;
	background-color: #eff0f1;
	background-image: url("/bgstrip.png");
	background-size: 100% 100%;
	background-repeat:repeat-y;
	background-position:center;
	font-family: Georgia, serif;
}

body{
	margin: 2% 11% 5% 10%;
	padding: 0;
	border-right 1px solid #0c192c;
	border-left 1px solid black;
	border-radius:1em;
	-moz-border-radius:1em;
	-webkit-border-radius:1em;
	box-shadow: 1em 1em 3em #0c192c;
	-moz-box-shadow: 1em 1em 3em #0c192c;
	-webkit-box-shadow: 1em 1em 3em #0c192c;
}

img.bg {
	width: 100%;
	height: 100%;
	position: absolute;
	top: 0;
	left: 0;
/*	z-index:-1;*/
}

a{
	color: #061717;
	font-style:italic;
	font-weight: bold;
	text-decoration: none;
}

a:hover {
	text-decoration:underline;
}

#con1{
	background-color:#0c192c;	/*#1d2b35;*/
	margin-bottom:0;
	color:white;
	padding: 2%;
	border-bottom: 4px solid DarkGrey;
	-moz-border-radius-topleft:1em;
	-webkit-border-top-left-radius:1em;
	-moz-border-radius-topright:1em;
	-webkit-border-top-right-radius:1em;
}

#header{
	margin:0;
	color:white;
	padding: 20px;
/*	border: 0 solid silver;
	border-radius:0.8em;
	--moz-border-radius:0.8em;
	--webkit-border-radius:0.8em;
*/
}

#header h3{
	text-align:center;
	font-family: "IM Fell DW Pica", helvetica, arial, serif;
	font-size: 4em;
}

#con2{

	background-color:AliceBlue;
	padding: 1em 0 0 0;
	border: 0 2px 0 2px solid silver;
	-moz-border-radius-bottomleft:1em;
	-webkit-border-bottom-left-radius:1em;
	-moz-border-radius-bottomright:1em;
	-webkit-border-bottom-right-radius:1em;
}

#mainbody{
	color:#061717;				/*DarkSlateGrey;*/
	padding: 2% 4%;
	line-height: 1.5em;
	font-size:170%;
/*	font-family: "Droid Serif", serif;*/
}

a.null {
	padding:6px;
	background-color:#ced4e0;
	font-weight:normal;
	font-style:normal;
	text-decoration:none;
}

h3 a:hover {
	font-weight:normal;
	text-decoration:none;
}

#links{
	margin: 0;
	padding: 0;
	background-color:AliceBlue;	/*#a0cecf;Gainsboro;*/
	overflow:auto;
	vertical-align:center;
}


#links ul {
	display:block;
	overflow:auto;
	margin:0;
	padding:0;
}


#links ul li{
	display:inline;
	margin:0;
	padding:0;
	clear:both;
	list-style-type:none;

}

#links ul li a{
	background-color:#ced4e0;
	font-style:normal;
	font-size: 170%;
	color: #0c192c;
	font-weight: bold;
	display:block;
	width: 12%;
	float:left;  
	margin-top:0;
	padding: 1% 1%;
	display:inline;
	text-align:center;
/*	outline: Grey solid 0.1em;*/
	text-decoration:none;
}

#links ul li a.current{
	background-color:AliceBlue;
	outline:0;
}


#links ul li:last-child a{
	border-right:0;
	width: 14%;
}

#links ul li a:hover {
	background-color:AliceBlue;
	text-decoration:underline;
	
}

#links ul li a:active {
}


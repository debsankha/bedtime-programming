div.ptitle {
	padding-left:2em;
	padding-bottom:1em;
	font-weight:bold;
	font-style:italic;
}

div.pbody {
	padding-left:2em;
	padding-bottom:1em;
}

div.foot {
	padding-left:2em;
	color:DarkSlateGray;
	font-style:italic;
}

span.x{
	background-color:CadetBlue;
}




#footer{
	text-align: center;
	font-size:120%;
	border-top: 1px solid grey;
	padding: 2% 3% 3% 2%;
}

#logos  img{
	border:0;
	margin:0;
	padding: 0 5%;
	display: inline;
	min-height:60px;
	height: 5%;
	width: auto;
}

#logos {
	text-align:center;
	padding:0;
	margin: 0 auto;
	margin-left: auto;
	margin-right: auto;
}
